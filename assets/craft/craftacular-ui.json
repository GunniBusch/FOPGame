{
  BitmapFont: {
    bold: {
      file: font-bold-export.fnt
    }
    font: {
      file: font-export.fnt
    }
    title: {
      file: font-title-export.fnt
    }
  }
  Color: {
    gray: {
      r: 0.55
      g: 0.55
      b: 0.55
      a: 1
    }
    green: {
      r: 0.03529522
      g: 0.77666664
      b: 0
      a: 1
    }
    white: {
      r: 1
      g: 1
      b: 1
      a: 1
    }
    a: { hex: "#2a0930ff" }
    s: { hex: "#2b2d30ff" }
  }
  TintedDrawable: {
    gray: {
      name: white
      color: {
        r: 0.73666674
        g: 0.73666674
        b: 0.73666674
        a: 1
      }
    }
  }
  "TiledDrawable": {
    "heart-tld": {
      "region": "heart",
      "color": "white",
      "minWidth": 18,
      "minHeight": 18
    },
    "heart-bg-tld": {
      "region": "heart-bg",
      "color": "white",
      "minWidth": 18,
      "minHeight": 18
    }
  }
  ButtonStyle: {
    default: {
      up: button
      over: button-hover
      disabled: button-disabled
    }
  }
  ImageButtonStyle: {
    default: {
      up: button
      over: button-hover
      disabled: button-disabled
    }
  }
  ImageTextButtonStyle: {
    default: {
      font: font
      up: button
      over: button-hover
      disabled: button-disabled
    }
  }
  LabelStyle: {
    default: {
      font: font
    }
    dim: {
      font: font
      fontColor: gray
    }
    bold: {
      font: bold
      background: underline
    }
    title: {
      font: title
    }
    xp: {
      font: bold
      fontColor: green
    }
  }
  ProgressBarStyle: {
    default-horizontal: {
      background: slider
      knobBefore: slider-knob
    }
    default-vertical: {
      background: slider
      knob: slider-knob
    }
    hunger: {
      background: meat-bg
      knobAfter: meat
    }
    health: {
      background: heart-bg-tld
      knobBefore: heart-tld
    }
    xp: {
      background: xp-bg
      knobBefore: xp
    }
    armor: {
      background: armor-bg
      knobBefore: armor
    }
  }
  ScrollPaneStyle: {
    default: {
      background: scroll-bg
      hScroll: scroll-horizontal
      hScrollKnob: scroll-knob-horizontal
      vScroll: scroll-vertical
      vScrollKnob: scroll-knob-vertical
    }
  }
  SliderStyle: {
    default-horizontal: {
      background: slider
      knob: slider-knob
    }
    default-vertical: {
      background: slider
      knob: slider-knob
    }
  }
  TextButtonStyle: {
    default: {
      font: font
      up: button
      over: button-hover
      disabled: button-disabled
    }
  }
  TextFieldStyle: {
    default: {
      font: font
      fontColor: white
      background: text-field
      cursor: cursor
      selection: gray
    }
  }
  TextTooltipStyle: {
    default: {
      label: default
      background: tooltip
    }
  }
  WindowStyle: {
    default: {
      background: window
      titleFont: font
    }
  }
}